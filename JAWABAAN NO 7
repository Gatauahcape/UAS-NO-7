public class SOALUASNO7 {
    public static void main(String[] args){
        int A = 5;
         
        for (int L = 0; L < A; L++) {
                
            for (int j = 1; j < A - L; j++) {
                System.out.print(" ");
            }                
            for (int B = 0; B <= L; B++) {
                System.out.print(B);
            }
            System.out.println();
        }
    }
}
